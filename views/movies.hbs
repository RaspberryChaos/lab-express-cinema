<h1>Ironhack Cinema</h1>
<h2>Click on the movie to check the showtimes!</h2>

<div class="movie-list flex">
    {{#each movie}}
    <div class="movie-card flex col">
        <img src="{{this.image}}" alt="movie poster">
        <h3>{{this.title}}</h3>
        <a href="/movies/{{this._id}}">See More</a>
    </div>
    {{/each}}
</div>

